

link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous")
link(rel='stylesheet' href='stylesheets/navigation.css')

header
    nav(id="navbar-custom" class='navbar navbar-expand-md nav-tabs')
        
        a(id="fuckingBootstrap" class='navbar-brand')
            img(class="logo" src='images/logoooo.png')
        
        div 
            ul(class="navbar-nav FNav test" id='testt')
                li(class="nav-item active ")
                    a(class="nav-link" href="/") Accueil  
                    span(class="sr-only") 
                li(class="nav-item")
                    a(class="nav-link" href="about") Présentation
                    span(class="sr-only") 
                li(class="nav-item")
                    a(class="nav-link" href="contact") Contact
                    span(class="sr-only")  
                li(class="nav-item")
                    a(class="nav-link num" href="tel:+330601079455") 0601079455
                li(class="nav-item dropdown FDropdown")
                    a(class="nav-link dropdown-toggle FDropdownSvg" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false") 
                        svg(xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-person-circle" viewBox="0 0 16 16")
                            path(d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z")
                            path(fill-rule="evenodd" d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1z")
                    ul(class="dropdown-menu" aria-labelledby="navbarDropdown")
                        unless user
                            a(class="dropdown-item" href="signup") Inscription
                            div(class="dropdown-divider")
                            a(class="dropdown-item" href="login") Connexion
                        else
                            a(class="dropdown-item" href="profile") Profil
                            div(class="dropdown-divider")
                            a(class="dropdown-item" href="#") Déconnection
                span(class="FFlex")

        div(class="hamburger" id="hamburger")
            div(class="bar1")
            div(class="bar2")
            div(class="bar3")
            

block script            
    script(src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js" integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB" crossorigin="anonymous")
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous")
    script(src="javascripts/navigation.js") 



